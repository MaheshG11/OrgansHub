<body class="hospitalRequestBG">

    <section  >
        <div class="container py-5 h-100 hospitalDatabaseOption">
          <div class="row d-flex justify-content-center align-items-center h-100">
            <div class="col-12 col-md-8 col-lg-6 col-xl-5">
              <div class="card bg-light text-white" style="border-radius: 1rem;">
                <div class="card-body p-5 text-center">
      
                  <div class="mb-md-5 mt-md-4 pb-5">
      
                    <form action="" method="post">
                    <div class="form-outline form-white mb-4">
                        <input type="tel" id="hospitalContactNo" placeholder="Contact No." class="form-control" />
                    </div>

                    <div class="form-outline form-white mb-4 ">
                        <input type="email" id="hospitalEmail" placeholder="Email" class="form-control" />
                    </div>
      
                    <div class="form-outline form-white mb-4">
                        <textarea class="form-control" id="exampleFormControlTextarea1" placeholder="Address" rows="3"></textarea>
                    </div>
      
                    <button class="btn  btn-lg px-5 hospitalRequestButton" type="submit" id="updateAddress">Update Address</button>
                </form>

      

      
                </div>
              </div>
            </div>
          </div>
        </div>
        </div>
      </section>
